{
  "name": "node-typescript-boilerplate",
  "version": "0.0.0",
  "description": "Minimalistic boilerplate to quick-start Node.js development in TypeScript.",
  "engines": {
    "node": ">= 12.13"
  },
  "devDependencies": {
    "@types/algoliasearch": "^3.34.11",
    "@types/cheerio": "^0.22.23",
    "@types/commander": "^2.12.2",
    "@types/jest": "~26.0.19",
    "@types/joi": "^14.3.4",
    "@types/koa": "^2.11.6",
    "@types/node": "~14.14.16",
    "@typescript-eslint/eslint-plugin": "~4.11.1",
    "@typescript-eslint/parser": "~4.11.1",
    "eslint": "~7.16.0",
    "eslint-config-prettier": "~7.1.0",
    "eslint-plugin-jest": "~24.1.3",
    "jest": "~26.6.3",
    "nodemon": "^2.0.6",
    "prettier": "~2.2.1",
    "rimraf": "~3.0.2",
    "ts-jest": "~26.4.4",
    "tsutils": "~3.17.0",
    "typescript": "~4.1.3"
  },
  "scripts": {
    "clean": "rimraf coverage build tmp",
    "build": "tsc -p tsconfig.release.json",
    "build:watch": "tsc -w -p tsconfig.release.json",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write \"**/*.ts\"",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "start": "node build/src/index.js",
    "start:dev": "nodemon build/src/index.js",
    "cli": "node build/src/cli.js",
    "docker:start": "docker-compose --file docker-compose.yml up",
    "docker:stop": "docker-compose --file docker-compose.yml stop",
    "docker:restart": "docker-compose --file docker-compose.yml restart",
    "docker:logs": "docker-compose --file docker-compose.yml logs"
  },
  "author": "Jakub Synowiec <github@jakubsynowiec.info>",
  "license": "Apache-2.0",
  "dependencies": {
    "@koa/cors": "^3.1.0",
    "@koa/router": "^10.0.0",
    "@types/koa-bodyparser": "^4.3.0",
    "algoliasearch": "^4.8.3",
    "amqplib": "^0.6.0",
    "apn": "^2.2.0",
    "chalk": "^4.1.0",
    "cheerio": "^1.0.0-rc.5",
    "commander": "^6.2.1",
    "dotenv": "^8.2.0",
    "glob": "^7.1.6",
    "http-status": "^1.5.0",
    "joi": "^17.3.0",
    "koa": "^2.13.0",
    "koa-bodyparser": "^4.3.0",
    "koa-logger": "^3.2.1",
    "lodash": "^4.17.20",
    "redis": "^3.0.2",
    "request": "^2.88.2",
    "request-promise": "^4.2.6",
    "signale": "^1.4.0",
    "tslib": "~2.0.3"
  }
}
